import random
import numpy as np


def stack_erschaffen():
  #stapel mit 25 random zahlen erschaffen
  stack = [random.randint(0, 100) for i in range(25)]

  #stapel sortieren
  stack.sort()

  return stack


anz_simulationen = 1000


stacks = []

# f√ºhrt die Simulation so oft aus wie in anz_simulationen angegeben und bautdementsprechend viele Listen
for i in range(anz_simulationen):
  stacks.append(stack_erschaffen())

# Wahrscheinlichkeit wie oft der erste Stapel vorkommt
gesuchter_stack = stacks[0]
vorkommen = sum(1 for stack in stacks if stack == gesuchter_stack)
wahrscheinlichkeit = vorkommen / anz_simulationen

print(wahrscheinlichkeit)
