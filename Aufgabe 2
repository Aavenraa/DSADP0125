from collections import deque
import random


def start_queue():

  queue = deque([random.randint(1, 1000) for i in range(1, 51)], maxlen = 50)

  return queue



def iteration(queue):

  for i in range(25):

    entferntes_elem = queue.popleft()


    naechstes_elem = 2 * entferntes_elem

    queue.append(naechstes_elem)


  return queue

queue = start_queue()

ergebnis = iteration(queue)

print(list(ergebnis))
